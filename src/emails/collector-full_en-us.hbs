---
layout: default_en-us.hbs
subject: (collector) Statement
---

<tr>
  <td>
    <table class="form">
      <thead>
      <tr>
        <th class="align-left" colspan="2">
          <div class="cover"><img src=<%= diagram.bg_image_url %>></div>
          <strong><%= survey.name %></strong>
        </th>
      </tr>
      </thead>
      <tbody>
      <tr>
        <td colspan="2"><span>Collector</span></td>
        <td><strong><%= collector.name %></strong></td>
      </tr>
      <tr>
        <td colspan="2"><span>Responses</span></td>
        <td><strong><%= collector.curr_amount %></strong></td>
      </tr>
      <tr>
        <td rowspan="2"><span>Reward</span></td>
        <td><strong><%= reward_name %></strong></td>
        <td><strong><%= reward_count %> / <%= reward_amount %></strong></td>
      </tr>
      <tr>
        <td><strong><%= reward_name %></strong></td>
        <td><strong><%= reward_count %> / <%= reward_amount %></strong></td>
      </tr>
      <tr>
        <td colspan="2"><span>Total (includes 15% service and 6% VAT)</span></td>
        <td><strong><%= reward_amount %></strong></td>
      </tr>
      </tbody>
    </table>
  </td>
</tr>
<tr>
  <td>
    {{> button type='primary' align='center' url='%surveys.data.grid%' title='See Responses'}}
  </td>
</tr>
